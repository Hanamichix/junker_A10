        <div id="newPost">
            <a href="newPost">
                <button class="btn btn-primary btn-lg">Post an Item</button>
            </a>
        </div>
        <ul style="list-style-type:none">
            {{#each posts}}
            <li>
                {{#if added}}
                <center>
                    <img class = "image" id = "add" src={{imageURL}} alt="thumb" class="thumbnail">
                </center>
                {{else}}
                <center>
                    <img class = "image" src={{imageURL}} alt="thumb" class="thumbnail">
                </center>
                {{/if}} 
            </li>
            <li>
                <h2>{{name}}</h2>
                <p class="desc">Description: {{description}} </p>                 
                <p class="desc">Itemid : {{itemid}}</p> 
            </li>
            {{/each}}
        </ul>
    </div>
    <script>

        $("#add").each(function() {  
            var imgsrc = this.src.replace("http://localhost:3000/", "");
            console.log("hello " + imgsrc);
            var dataImage = localStorage.getItem(imgsrc);
            console.log(dataImage);
            this.src = "data:image/png;base64," + dataImage;
        }); 

    </script>

